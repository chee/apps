THIS IS IT:

<?xml version="1.0" encoding="UTF-8"?>
<kit>
	<defaultParams></defaultParams>
	<soundSources>
		<sound name="kick">
			<osc1
					type="sample"
					fileName="SAMPLES/op1kits/acoustic_toy/01-kick.wav">
			</osc1>
			<defaultParams
					oscBVolume="0x80000000"
					lpfFrequency="0x7FFFFFFF">
			</defaultParams>
		</sound>
	</soundSources>
	<selectedDrumIndex>1</selectedDrumIndex>
</kit>
